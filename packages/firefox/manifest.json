{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "26.1.16.2",
  "description": "__MSG_extDescription__",
  "permissions": [
    "*://*.iota/",
    "*://*.iota/*",
    "omnibox",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "action": {
    "default_title": "IOTA Names Resolver",
    "default_popup": "popup.html"
  },
  "omnibox": {
    "keyword": "iota"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "iota-names-resolver@example.com",
      "strict_min_version": "109.0"
    }
  }
}
