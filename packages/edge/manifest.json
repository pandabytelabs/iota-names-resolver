{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "26.1.15.0",
  "description": "__MSG_extDescription__",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "omnibox"
  ],
  "host_permissions": [
    "*://*.iota/*",
    "*://*.iota"
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "IOTA Names Resolver",
    "default_popup": "popup.html"
  },
  "omnibox": {
    "keyword": "iota"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "minimum_edge_version": "110.0"
}
